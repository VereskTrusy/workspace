<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.Regions.dao.RegionsDAO">
	<select id="selectRegionsList" resultType="vo.RegionsVO">
		SELECT 
			REGION_ID
			, REGION_NAME
		FROM REGIONS
	</select>
	
	<select id="selectRegion" parameterType="String" resultType="vo.RegionsVO">
		SELECT
			REGION_ID
			, REGION_NAME
		FROM REGIONS
		WHERE REGION_ID = #{regionId}
	</select>
	
	
	
</mapper>